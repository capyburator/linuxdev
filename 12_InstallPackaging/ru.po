#: guess-number.c:28
msgid "Program that guesses a number"
msgstr "Программа, угадываюшая ваше число"

#: guess-number.c:29
msgid "USAGE: [LANGUAGE=ru_RU] ./guess-number [OPTIONS]"
msgstr "Использование: [LANGUAGE=ru_RU] ./guess-number [ПАРАМЕТРЫ]"

#: guess-number.c:30
msgid "OPTIONS:"
msgstr "ПАРАМЕТРЫ:"

#: guess-number.c:31
msgid "   -r, --roman         Print numbers in roman notation"
msgstr "   -r, --roman         Печать чисел в римской системе счисления"

#: guess-number.c:32
msgid "   -h, --help          Print help"
msgstr "   -h, --help          Показать справку"

#: guess-number.c:70
msgid "Unrecognized option, try -h or --help"
msgstr "Неизвестный параметр, запустите с -h или --help для справки"

#: guess-number.c:76
#, c-format
msgid "Pick a number within [%s, "
msgstr "Загадайте число в диапазоне [%s, "

#: guess-number.c:80
#, c-format
msgid "Pick a number within [%d, %d]\n"
msgstr "Загадайте число в диапазоне [%d, %d]\n"

#: guess-number.c:89
#, c-format
msgid "Is your number greater than %s? [y/n]: "
msgstr "Ваше число больше, чем %s? [д/н]: "

#: guess-number.c:91
#, c-format
msgid "Is your number greater than %d? [y/n]: "
msgstr "Ваше число больше, чем %d? [д/н]: "

#: guess-number.c:97
msgid "y"
msgstr "д"

#: guess-number.c:98
msgid "n"
msgstr "н"

#: guess-number.c:100
#, c-format
msgid "Type `yes` or `no`: "
msgstr "Введите `да` или `нет`: "

#: guess-number.c:109
#, c-format
msgid "Your number is %s.\n"
msgstr "Ваше число равно %s.\n"

#: guess-number.c:111
#, c-format
msgid "Your number is %d.\n"
msgstr "Ваше число равно %d.\n"
